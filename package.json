  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.14.1",
    "allure-playwright": "^3.2.1",
    "rimraf": "^6.0.1"
  }
}
